{
  "cli": {
    "version": ">= 7.6.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "preview"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_API_URL": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}