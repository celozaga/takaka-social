{
  "expo": {
    "name": "Takaka",
    "slug": "takaka",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "scheme": "takaka",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "social.takaka.app",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to allow you to take and upload photos and videos to your posts.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to allow you to select and upload photos and videos for your posts.",
        "NSMicrophoneUsageDescription": "This app uses the microphone to allow you to record videos with audio to upload to your posts."
      }
    },
    "android": {
      "package": "social.takaka.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      "expo-notifications",
      "expo-video",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f2688b27-fa00-41bb-9c4b-9c8636d5ebb9"
      }
    }
  }
}
